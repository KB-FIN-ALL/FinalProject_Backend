<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">



<mapper namespace="com.kb.store.mapper.StoreMapper">
    <resultMap id="StoreResultMap" type="com.kb.store.dto.StoreDTO">
        <id property="storeId" column="store_id"/>
        <result property="cpId" column="cp_id"/>
        <result property="stdId" column="std_id"/>
        <result property="storeDate" column="store_date"/>
        <result property="quantity" column="quantity"/>
    </resultMap>

    <select id="selectStore" resultMap="StoreResultMap">
        select * from store;
    </select>
</mapper>
